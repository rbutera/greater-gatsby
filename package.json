  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "npm-run-all --parallel develop:gatsby storybook",
    "develop:gatsby": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "serve --no-clipboard --listen 8899",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm-run-all --sequential --print-name test:unit:ci test:e2e:ci",
    "test:unit:ci": "jest",
    "test:unit:dev": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:ci": " start-server-and-test develop:gatsby http://localhost:8000 cy:run",
    "test:e2e:dev": "start-server-and-test develop:gatsby http://localhost:8000 cy:dev",
    "cy:run": "cypress run",
    "cy:dev": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbutera/greater-gatsby"
  },
  "bugs": {
    "url": "https://github.com/rbutera/greater-gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix",
      "jest --findRelatedTests"
    ],
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
